classdef HBAval
	
	linked byte val0
	linked byte val1
	linked byte val2
	linked byte val3
	linked byte val4
	linked byte val5
	
	func Create (HBAval : int low, int_s high)
		HBAval ret = sys Program.alloc($LinkedClassSize)
		ret.val4 = high & 0xFF
		ret.val5 = (high >> 8)
		ret.val0 = low & 0xFF
		ret.val1 = (low >> 8) & 0xFF
		ret.val2 = (low >> 16) & 0xFF
		ret.val3 = (low >> 24) & 0xFF
		return ret
	